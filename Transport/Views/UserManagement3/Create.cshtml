@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

    <script>

        $('#dugme').click(function () {

            var email = $('#Email').val();
            var phonenumber = $('#PhoneNumber').val();
            var username = $('#UserName').val();
            var firstname = $('#FirstName').val();
            var lastname = $('#LastName').val();
            var address = $('#Address').val();
            var street = $('#StreetNumber').val();
            var city = $('#City').val();
            var country = $('#IDCountry option:selected').text();
            var status = $('#Active option:selected').text();

            //$('#paragraf').html("naziv: " + naziv + " kolicina: " + kolicina + " jedinica: " + jedinica + " barkode: " + barkode + " pakovanje: " + pakovanje + " dobavljac: " + dobavljac);

            $.ajax({
                url: '/UserManagement3/Create',
                type: 'POST',
                //data: "Naziv=" + naziv + "&" + "Kolicina=" + kolicina + "&" + "JedinicaMjere=" + jedinica + "&" + "BarKode=" + barkode + "&" + "IDAmbalaže=" + pakovanje + "&" + "IDDobavljača=" + dobavljac ,
                //dataType: JSON,
                data: {
                    Email: email,
                    PhoneNumber: phonenumber,
                    UserName: username,
                    FirstName: firstname,
                    LastName: lastname,
                    Address: address,
                    StreetNumber: street,
                    City: city,
                    IDCountry: country,
                    Active: status,
                },
                success: function (data, status, xhr) {
                    if (data.success) {
                        alert(data.Email + " " + data.PhoneNumber + " " + data.UserName + " " + data.FirstName + " " + data.LastName + " " + data.Address + " " + data.StreetNumber + " " + data.City + " " + data.IDCountry + " " + data.Active + " "   )
                    } else {
                        console.log(data);

                        for (var err of data.error) {
                            var field = err.Key;
                            var errField = $('[data-valmsg-for="' + field + '"]')
                            errField.html(err.Value.join("<br>"));
                        }

                    }

                },
                error: function (jqXhr, textStatus, errorMessage) {
                    alert(errorMessage);

                }
            });

        });

    </script>
}
